(Minimally Invasive Surgical Procedures[mesh:noexp] OR Endovascular Procedures[mesh:noexp] OR Endoscopy[mesh] OR Angioscopy[mesh] OR Vascular Surgical Procedures[mesh:noexp] OR Saphenous Vein[mesh:noexp] OR (Veins[mesh:noexp] AND Leg[mesh:noexp]) OR SEPS[Title/Abstract] OR (endoscop*[Title/Abstract] AND (surger*[Title/Abstract] OR surgic*[Title/Abstract] OR ligation*[Title/Abstract] OR ligatur*[Title/Abstract] OR ablation*[Title/Abstract])) OR (perforat*[Title/Abstract] AND (endoscop*[Title/Abstract] OR subfascial[Title/Abstract])))